@page
@model EvetWebsite.Areas.Admin.Pages.PreorderPage.EditModel

@{
    ViewData["Title"] = "Edit";
}

<div class="card card-default">
    <div class="card-header">
        <h3 class="card-title">Update Pre Order Form on Gender and Development in Nigeria: Concepts, Issues and Strategies by Prof. O.J Para-Mallam, mni</h3>

    </div>
    <!-- /.card-header -->

    <div class="card-body">
        <div class="row">
            <div class="col-md-12">
                <form method="post">
                    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                    <input type="hidden" asp-for="PreorderForm.Id" />
                    <div class="form-group">
                        <label asp-for="PreorderForm.Email" class="control-label"></label>
                        <input asp-for="PreorderForm.Email" class="form-control" />
                        <span asp-validation-for="PreorderForm.Email" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="PreorderForm.FirstName" class="control-label"></label>
                        <input asp-for="PreorderForm.FirstName" class="form-control" />
                        <span asp-validation-for="PreorderForm.FirstName" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="PreorderForm.LastName" class="control-label"></label>
                        <input asp-for="PreorderForm.LastName" class="form-control" />
                        <span asp-validation-for="PreorderForm.LastName" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="PreorderForm.PhoneNumber" class="control-label"></label>
                        <input asp-for="PreorderForm.PhoneNumber" class="form-control" />
                        <span asp-validation-for="PreorderForm.PhoneNumber" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="PreorderForm.StreetAddress" class="control-label"></label>
                        <input asp-for="PreorderForm.StreetAddress" class="form-control" />
                        <span asp-validation-for="PreorderForm.StreetAddress" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="PreorderForm.City" class="control-label"></label>
                        <input asp-for="PreorderForm.City" class="form-control" />
                        <span asp-validation-for="PreorderForm.City" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="PreorderForm.State" class="control-label"></label>
                        <input asp-for="PreorderForm.State" class="form-control" />
                        <span asp-validation-for="PreorderForm.State" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="PreorderForm.ZipCode" class="control-label"></label>
                        <input asp-for="PreorderForm.ZipCode" class="form-control" />
                        <span asp-validation-for="PreorderForm.ZipCode" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="PreorderForm.Country" class="control-label"></label>
                        <input asp-for="PreorderForm.Country" class="form-control" />
                        <span asp-validation-for="PreorderForm.Country" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="PreorderForm.Quantity" class="control-label"></label>
                        <input asp-for="PreorderForm.Quantity" class="form-control" />
                        <span asp-validation-for="PreorderForm.Quantity" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <input type="submit" value="Submit" class="btn btn-primary" />
                        <a asp-page="./Index" class="btn bg-gradient-warning btn-sm" style="float:right;">Back</a>

                    </div>
                </form>
            </div>
        </div>


        <!-- /.row -->
    </div>
    <!-- /.card-body -->
    <div class="card-footer">
    </div>
</div>


